  "devDependencies": {
    "doctoc": "^0.7.1",
    "nan": "~1.2.0",
    "nave": "~0.4.3",
    "semver": "~2.3.1",
    "tap": "~0.4.11"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/v8-flags/blob/master/LICENSE"
